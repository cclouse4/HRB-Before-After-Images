{
	"name": "hrb/before-after-images",
	"category": "common",
	"attributes": {
		"align": {
			"type": "string"
		},
		"beforeId": {
			"type": "number"
		},
		"beforeUrl": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-hrb-before-after-images__before-image img",
			"attribute": "src"
		},
		"beforeAlt": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-hrb-before-after-images__before-image img",
			"attribute": "alt",
			"default": ""
		},
		"afterId": {
			"type": "number"
		},
		"afterUrl": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-hrb-before-after-images__after-image img",
			"attribute": "src"
		},
		"afterAlt": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-hrb-before-after-images__after-image img",
			"attribute": "alt",
			"default": ""
		},
		"dividerLocation": {
			"type": "number",
			"default": 50
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": "figcaption"
		},
		"isVertical": {
			"type": "boolean",
			"default": false
		},
		"sizeSlug": {
			"type": "string"
		}
	}
}
